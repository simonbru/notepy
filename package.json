{
  "name": "notepy",
  "version": "0.0.0",
  "scripts": {
    "build": "rm app/static/dist -rf && webpack -p",
    "build:dev": "rm app/static/dist -rf && webpack --mode=development",
    "clean": "rm app/static/dist -rf",
    "lint": "eslint app/js",
    "start": "webpack-dev-server --mode=development",
    "watch": "webpack --watch -p --progress",
    "watch:dev": "webpack --watch --mode=development --progress"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:simonbru/notepy.git"
  },
  "author": "Simon Brulhart",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "core-js": "^2.5.7",
    "css-loader": "^2.1.1",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.7.0",
