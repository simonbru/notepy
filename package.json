    "webpack": "^4.26.0",
    "webpack-cli": "^3.1.2",
    "webpack-manifest-plugin": "^2.0.4"
  },
  "dependencies": {
    "bootstrap-sass": "^3.4.1",
    "jquery": "^3.3.1",
    "react": "^16.8.4",
